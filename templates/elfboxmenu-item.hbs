<div class="menu-item-inner {{#if _locked}}locked{{/if}}" aria-label="{{_globals._menu._elfBoxMenu.menuItem}}" {{#if _globals._menu._elfBoxMenu.menuItem}}tabindex="0"{{/if}}>
    <div class="menu-item-graphic">
        {{#if _graphic.src}}
            {{#if _isLocked}}
            <img src="{{_graphic.locked}}" alt="{{_graphic.alt}}" title="Module is locked, You need to finish the previous module to unlock this one."/>
                {{else}}
                <a href="#/id/{{_id}}" class="{{#if _isVisited}}visited{{/if}}">
                    <img src="{{_graphic.src}}" alt="{{_graphic.alt}}" />
                </a>
            {{/if}}
        {{/if}}
    </div>
    <div class="menu-item-title">
        <div class="menu-item-title-inner h3 accessible-text-block" role="heading" aria-level="2" tabindex="0">{{{displayTitle}}}</div>
    </div>
    <div class="menu-item-body">
        <div class="menu-item-body-inner">{{{a11y_text body}}}</div>
    </div>
    <div class="menu-item-button">
        <button aria-label="{{linkText}} {{#if _isVisited}}{{_globals._accessibility._ariaLabels.visited}}{{/if}}" class="{{#if _isVisited}}visited{{/if}} {{#if _isLocked}}disabled{{/if}} accessible-text-block" {{#if _isLocked}}disabled="disabled"{{/if}}>
            {{#if _isLocked}}
            {{{linkLockedText}}}
                {{else}}
                {{#if _isVisited}}
                    {{{linkVisitedText}}}
                    {{else}}
                    {{{linkText}}}
                {{/if}}
            {{/if}}
        </button>
        {{#if duration}}
            <span class="menu-item-duration accessible-text-block" role="region" tabindex="0">{{#if _globals._menu._elfBoxMenu.durationLabel}}{{{_globals._menu._elfBoxMenu.durationLabel}}}{{/if}} {{{duration}}}</span>
        {{/if}}
    </div>
</div>
